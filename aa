DROP TABLE IF EXISTS `eb_question`;
/*!40101 SET @saved_cs_client     = @@character_set_client */;
/*!40101 SET character_set_client = utf8 */;
CREATE TABLE `eb_question` (
  `id` int(10) unsigned NOT NULL AUTO_INCREMENT,
  `title` text,
  `answer` varchar(255) DEFAULT NULL,
  `create_time` datetime DEFAULT NULL,
  `problem_id` int(11) DEFAULT NULL,
  `is_adopt` int(10) unsigned DEFAULT '0',
  `description` text,
  `question_type` varchar(20) DEFAULT NULL COMMENT 'dadongzuo,jingxidongzuo,yuyan,renzhi,shehuihuodong',
  PRIMARY KEY (`id`),
  KEY `Index_2` (`problem_id`)
) ENGINE=MyISAM AUTO_INCREMENT=733 DEFAULT CHARSET=utf8;
/*!40101 SET character_set_client = @saved_cs_client */;

