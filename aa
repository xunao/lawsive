--
-- Table structure for table `eb_position`
--

DROP TABLE IF EXISTS `eb_position`;
/*!40101 SET @saved_cs_client     = @@character_set_client */;
/*!40101 SET character_set_client = utf8 */;
CREATE TABLE `eb_position` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `source_id` int(11) NOT NULL COMMENT '内容ID',
  `source_type` varchar(256) NOT NULL DEFAULT 'news' COMMENT '内容类型，默认是新闻',
  `priority` int(11) NOT NULL DEFAULT '100' COMMENT '内容在该位置的优先级',
  `pos_name` varchar(256) NOT NULL COMMENT '位置名',
  PRIMARY KEY (`id`)
) ENGINE=MyISAM AUTO_INCREMENT=4 DEFAULT CHARSET=utf8 COMMENT='位置内容管理';
/*!40101 SET character_set_client = @saved_cs_client */;

--
-- Dumping data for table `eb_position`
--

