--
-- Table structure for table `eb_problem`
--

DROP TABLE IF EXISTS `eb_problem`;
/*!40101 SET @saved_cs_client     = @@character_set_client */;
/*!40101 SET character_set_client = utf8 */;
CREATE TABLE `eb_problem` (
  `id` int(10) NOT NULL AUTO_INCREMENT,
  `name` varchar(255) DEFAULT NULL,
  `start_month` int(11) DEFAULT NULL COMMENT '宝宝月份开始',
  `end_month` int(11) DEFAULT NULL COMMENT '宝宝月份结束',
  `limit_time` int(11) DEFAULT NULL,
  `point` int(11) DEFAULT '10',
  `type` varchar(45) DEFAULT NULL,
  `create_time` datetime DEFAULT NULL,
  `is_adopt` int(10) unsigned DEFAULT '0',
  `category_id` int(10) unsigned DEFAULT NULL,
  `click_count` int(10) unsigned DEFAULT '0',
  `photo_url` varchar(255) DEFAULT NULL,
  `priority` int(11) NOT NULL,
  `description` text NOT NULL,
  `problem_type` int(11) NOT NULL DEFAULT '1' COMMENT '测评类型，1：宝宝测评，2：父母测评',
  PRIMARY KEY (`id`)
) ENGINE=MyISAM AUTO_INCREMENT=61 DEFAULT CHARSET=utf8;
/*!40101 SET character_set_client = @saved_cs_client */;

