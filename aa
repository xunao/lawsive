  `uid` mediumint(8) NOT NULL DEFAULT '0',
  `dateline` int(10) NOT NULL DEFAULT '0',
  `newthreads` mediumint(8) NOT NULL DEFAULT '0',
  PRIMARY KEY (`fid`,`uid`)
) ENGINE=MyISAM DEFAULT CHARSET=utf8;
